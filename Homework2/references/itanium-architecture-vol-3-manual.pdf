    <script type="text/javascript" language="JavaScript">
      function addfragment (form, fragment) {
        var   input = document.createElement ('input');
        input.type  = 'hidden';
        input.name  = 'fragment';
        input.value = fragment;
        form.appendChild (input);          
      }
      var href = document.location.href;
      var frag = null;
      if (match = href.match (/\#(.*)$/)) { // fragment
        frag = match [1];
      }
      if (document.loginform) {
        if (window.self != window.top) { // Not top level window
          var   inframe = document.createElement ('input');
          inframe.type  = 'hidden';
          inframe.name  = 'inframe';
          inframe.value = 1;
          document.loginform.appendChild (inframe);
        }
        if (frag) {
          addfragment (document.loginform, frag);
        }
        document.loginform.username.focus ();
      }
    </script>  
  
